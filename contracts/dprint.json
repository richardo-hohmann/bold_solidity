{
  "typescript": {
    "useBraces": "always"
  },
  "json": {
  },
  "markdown": {
  },
  "includes": ["**/*.{ts,js,mjs,json,md}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "/artifacts",
    "/broadcast",
    "/cache",
    "/lib",
    "/out"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.90.0.wasm",
    "https://plugins.dprint.dev/json-0.19.2.wasm",
    "https://plugins.dprint.dev/markdown-0.16.4.wasm"
  ]
}
